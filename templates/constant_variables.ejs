                </div>
                <div class="col-md-6">
                  <input type="text" class="form-control input-sm" name="AVOID_SETTING_SYNC_START" value ="<%= constant_variables.AVOID_SETTING_SYNC_START%>">
                </div>
              </div>
              <div class="form-group">
                <div class="col-md-4 col-md-offset-1 visible">
                  <label for="AVOID_SETTING_SYNC_END" class="control-label">Avoid Setting Sync End</label>
                </div>
                <div class="col-md-6">
                  <input type="text" class="form-control input-sm" name="AVOID_SETTING_SYNC_END" value ="<%= constant_variables.AVOID_SETTING_SYNC_END%>">
                </div>
              </div>
            </div>

            <div role="tabpanel" class="tab-pane fade in" id="delivery_areas_sync_panel">
              <div class="form-group">
                <div class="col-md-4 col-md-offset-1 visible">
                  <label for="AVOID_DELIVERY_AREAS_SYNC" class="control-label">Avoid Delivery Areas Sync</label>
                </div>
                <div class="col-md-6">
                  <input type="hidden" name="AVOID_DELIVERY_AREAS_SYNC" value="off">
                  <input type="checkbox" class="form-control input-sm" name="AVOID_DELIVERY_AREAS_SYNC" <% if(constant_variables.AVOID_DELIVERY_AREAS_SYNC=="true"){%> <%= "checked" %> <% }%> >
