              </div>
            </div>

            <div role="tabpanel" class="tab-pane fade in" id="settings_sync_panel">
              <div class="form-group">
                <div class="col-md-4 col-md-offset-1 visible">
                  <label for="AVOID_SETTING_SYNC" class="control-label">Avoid Setting Sync</label>
                </div>
                <div class="col-md-6">
                  <input type="hidden" name="AVOID_SETTING_SYNC" value="off">
                  <input type="checkbox" class="form-control input-sm" name="AVOID_SETTING_SYNC" <% if(constant_variables.AVOID_SETTING_SYNC=="true"){%> <%= "checked" %> <% }%> >
