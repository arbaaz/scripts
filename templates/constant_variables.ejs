                <div class="col-md-6">
                  <input type="text" class="form-control input-sm" name="AVOID_MENU_SYNC_START" value ="<%= constant_variables.AVOID_MENU_SYNC_START%>">
                </div>
              </div>
              <div class="form-group">
                <div class="col-md-4 col-md-offset-1 visible">
                  <label for="AVOID_MENU_SYNC_END" class="control-label">Avoid Menu Sync End</label>
                </div>
                <div class="col-md-6">
                  <input type="text" class="form-control input-sm" name="AVOID_MENU_SYNC_END" value ="<%= constant_variables.AVOID_MENU_SYNC_END%>">
