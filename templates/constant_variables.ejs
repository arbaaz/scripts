              <label for="CALL_BEFORE_ORDER_ACTION" class="control-label">Call Before Order Action</label>
            </div>
            <div class="col-md-6">
              <input type="hidden" name="CALL_BEFORE_ORDER_ACTION" value="off">
              <input type="checkbox" class="form-control input-sm" name="CALL_BEFORE_ORDER_ACTION" <% if(constant_variables.CALL_BEFORE_ORDER_ACTION=="true"){%> <%= "checked" %> <% }%> >
            </div>
          </div>

          <div class="form-group">
            <div class="col-md-4 col-md-offset-1 visible">
              <label for="ORDER_COUNTER_NUMBER" class="control-label">Order Counter Number</label>
