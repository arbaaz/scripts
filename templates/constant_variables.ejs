          <div class ="col-md-offset-1">
            <ul class="nav nav-pills nav-justified" role="tablist" id="myTab">
              <li class="active" role="presentation" id="settings_sync"><a href="#release_update_panel" role="tab" data-toggle="tab">Avoid Release Update Sync</a></li>
              <li  role="presentation" id="location_sync"><a href="#settings_sync_panel" role="tab" data-toggle="tab">Avoid Settings Sync</a></li>
              <li role="presentation" id="menu_sync"><a href="#delivery_areas_sync_panel" role="tab" data-toggle="tab">Avoid Delivery Areas Sync</a></li>
              <li role="presentation" id="profile_sync"><a href="#menu_sync_panel" role="tab" data-toggle="tab">Avoid Menu Sync</a></li>
              <li role="presentation" id="profile_sync"><a href="#avoid_sync_panel" role="tab" data-toggle="tab">Avoid Sync</a></li>
            </ul>
            <br>
          </div>
          <div class="tab-content">
            <div role="tabpanel" class="tab-pane fade in active" id="release_update_panel">
              <div class="form-group">
                <div class="col-md-4 col-md-offset-1 visible">
                  <label for="AVOID_RELEASE_UPDATE_SYNC" class="control-label">Avoid Release Update Sync</label>
                </div>
                <div class="col-md-6">
                  <input type="hidden" name="AVOID_RELEASE_UPDATE_SYNC" value="off">
                  <input type="checkbox" class="form-control input-sm" name="AVOID_RELEASE_UPDATE_SYNC" <% if(constant_variables.AVOID_RELEASE_UPDATE_SYNC=="true"){%> <%= "checked" %> <% }%> >
                </div>
              </div>
              <div class="form-group">
                <div class="col-md-4 col-md-offset-1 visible">
                  <label for="AVOID_RELEASE_UPDATE_SYNC_START" class="control-label">Avoid Release Update Sync Start</label>
                </div>
                <div class="col-md-6">
                  <input type="text" class="form-control input-sm" name="AVOID_RELEASE_UPDATE_SYNC_START" value ="<%= constant_variables.AVOID_RELEASE_UPDATE_SYNC_START%>">
                </div>
              </div>
              <div class="form-group">
                <div class="col-md-4 col-md-offset-1 visible">
                  <label for="AVOID_RELEASE_UPDATE_SYNC_END" class="control-label">Avoid Release Update Sync End</label>
                </div>
                <div class="col-md-6">
                  <input type="text" class="form-control input-sm" name="AVOID_RELEASE_UPDATE_SYNC_END" value ="<%= constant_variables.AVOID_RELEASE_UPDATE_SYNC_END%>">
