                <div class="col-md-6">
                  <input type="text" class="form-control input-sm" name="AVOID_DELIVERY_AREAS_SYNC_START" value ="<%= constant_variables.AVOID_DELIVERY_AREAS_SYNC_START%>">
                </div>
              </div>
              <div class="form-group">
                <div class="col-md-4 col-md-offset-1 visible">
                  <label for="AVOID_DELIVERY_AREAS_SYNC_END" class="control-label">Avoid Delivery Areas Sync End</label>
                </div>
                <div class="col-md-6">
                  <input type="text" class="form-control input-sm" name="AVOID_DELIVERY_AREAS_SYNC_END" value ="<%= constant_variables.AVOID_DELIVERY_AREAS_SYNC_END%>">
                </div>
              </div>
            </div>

            <div role="tabpanel" class="tab-pane fade in" id="menu_sync_panel">
              <div class="form-group">
                <div class="col-md-4 col-md-offset-1 visible">
                  <label for="AVOID_MENU_SYNC" class="control-label">Avoid Menu Sync</label>
                </div>
                <div class="col-md-6">
                  <input type="hidden" name="AVOID_MENU_SYNC" value="off">
                  <input type="checkbox" class="form-control input-sm" name="AVOID_MENU_SYNC" <% if(constant_variables.AVOID_MENU_SYNC=="true"){%> <%= "checked" %> <% }%> >
                </div>
              </div>
              <div class="form-group">
                <div class="col-md-4 col-md-offset-1 visible">
                  <label for="AVOID_MENU_SYNC_START" class="control-label">Avoid Menu Sync Start</label>
