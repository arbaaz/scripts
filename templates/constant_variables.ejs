            <div class="col-md-6">
              <input type="text" class="form-control input-sm" name="RESTAURANT_RUSH_TIME_SLAB" value ="<%= constant_variables.RESTAURANT_RUSH_TIME_SLAB%>">
            </div>
          </div>
          <div class="form-group">
            <div class="col-md-4 col-md-offset-1 visible">
              <label for="RESTAURANT_RUSH_ORDER_COUNT" class="control-label">Restaurant Rush Order Count</label>
            </div>
            <div class="col-md-6">
              <input type="text" class="form-control input-sm" name="RESTAURANT_RUSH_ORDER_COUNT" value ="<%= constant_variables.RESTAURANT_RUSH_ORDER_COUNT%>">
            </div>
          </div>
          <div class="form-group">
            <div class="col-md-4 col-md-offset-1 visible">
              <label for="RESTAURANT_RUSH_ERROR_MESSAGE" class="control-label">Restaurant Rush Error Message</label>
            </div>
            <div class="col-md-6">
              <input type="text" class="form-control input-sm" name="RESTAURANT_RUSH_ERROR_MESSAGE" value ="<%= constant_variables.RESTAURANT_RUSH_ERROR_MESSAGE%>">
            </div>
          </div>
          <div class="form-group">
            <div class="col-md-4 col-md-offset-1 visible">
              <label for="RESTAURANT_INVISIBLE_ERROR_MESSAGE" class="control-label">Restaurant Invisible Error Message</label>
            </div>
            <div class="col-md-6">
              <input type="text" class="form-control input-sm" name="RESTAURANT_INVISIBLE_ERROR_MESSAGE" value ="<%= constant_variables.RESTAURANT_INVISIBLE_ERROR_MESSAGE%>">
            </div>
          </div>
          <div class="form-group">
            <div class="col-md-4 col-md-offset-1 visible">
              <label for="RESTAURANT_INACTIVE_ERROR_MESSAGE" class="control-label">Restaurant Inactive Error Message</label>
            </div>
            <div class="col-md-6">
              <input type="text" class="form-control input-sm" name="RESTAURANT_INACTIVE_ERROR_MESSAGE" value ="<%= constant_variables.RESTAURANT_INACTIVE_ERROR_MESSAGE%>">
            </div>
          </div>
          <div class="form-group">
            <div class="col-md-4 col-md-offset-1 visible">
              <label for="PROMOTION_INVALID_ERROR_MESSAGE" class="control-label">Promotion Invalid Error Message</label>
            </div>
            <div class="col-md-6">
              <input type="text" class="form-control input-sm" name="PROMOTION_INVALID_ERROR_MESSAGE" value ="<%= constant_variables.PROMOTION_INVALID_ERROR_MESSAGE%>">
            </div>
          </div>
          <div class="form-group">
            <div class="col-md-4 col-md-offset-1 visible">
              <label for="OPEN_LOGO_SIZE" class="control-label">Open Logo Size</label>
            </div>
            <div class="col-md-6">
              <input type="text" class="form-control input-sm" name="OPEN_LOGO_SIZE" value ="<%= constant_variables.OPEN_LOGO_SIZE%>">
