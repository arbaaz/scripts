    <div class="panel">
      <div class="panel-body">
        <form role="form" class="form-horizontal" id="android_settings_form" method="POST">
          <div class="form-group">
            <div class="col-md-2 col-md-offset-9">
              <a class="btn btn-info btn-sm" href="#android-releases">Android Releases <i class="fa fa-arrow-right"></i></a>
            </div>
          </div>
          <div class="form-group">
            <div class="col-md-4 col-md-offset-1">
              <label for="SETTINGS_FOR_APP_VERSION" class="control-label">App Version</label>
            </div>
            <div class="col-md-6">
              <select class="form-control input-sm" name="current_version" id = "android_versions">
              <% _.each(android_settings.versions,function(version) { %>
                <option value= "<%=version%>" <%= current_version == version ? 'selected' : '' %> ><%=version%></option>
              <%}); %>
              </select>
            </div>
          </div>
          <div class="form-group">
            <div class="col-md-4 col-md-offset-1">
              <label for="RESTAURANT_NUMBER_SYNC_LIMIT" class="control-label">Restaurant Number Sync Limit</label>
            </div>
