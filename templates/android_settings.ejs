              <input type="text" class="form-control input-sm" name="RESTAURANT_NUMBER_SYNC_LIMIT"  value ="<%=android_settings.RESTAURANT_NUMBER_SYNC_LIMIT %>" >
            </div>
          </div>
          <div class="form-group">
            <div class="col-md-4 col-md-offset-1">
